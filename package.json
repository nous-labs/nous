{
  "name": "@nouslabs/sdk",
  "version": "1.4.0",
  "description": "@nouslabs/sdk (forget what you know) - TypeScript SDK for Qubic blockchain",
  "main": "./index.ts",
  "module": "./index.ts",
  "types": "./index.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.ts",
      "types": "./index.ts"
    },
    "./clients": {
      "import": "./src/clients/index.ts",
      "types": "./src/clients/index.ts"
    },
    "./types": {
      "import": "./src/types/index.ts",
      "types": "./src/types/index.ts"
    },
    "./utils": {
      "import": "./src/utils/index.ts",
      "types": "./src/utils/index.ts"
    },
    "./react/query": {
      "import": "./src/react/query/index.ts",
      "types": "./src/react/query/index.ts"
    }
  },
  "files": [
    "src",
    "index.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "build": "bun build ./index.ts --outdir ./dist --target node",
    "dev": "bun run index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "prepublishOnly": "bun test && bun run typecheck"
  },
  "keywords": [
    "qubic",
    "blockchain",
    "crypto",
    "smart-contracts",
    "typescript",
    "sdk",
    "api",
    "web3",
    "dlt",
    "distributed-ledger",
    "navia",
    "qubic"
  ],
  "author": "Nous Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nous-labs/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/nous-labs/sdk/issues"
  },
  "homepage": "https://github.com/nous-labs/sdk#readme",
  "dependencies": {
    "@noble/hashes": "^1.4.0",
    "@tanstack/react-query": "^5.90.5",
    "@walletconnect/sign-client": "^2.22.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/bun": "latest",
    "@types/react": "^18.3.0",
    "happy-dom": "^20.0.8",
    "node-fetch": "^3.3.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "@tanstack/react-query": ">=5.0.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
